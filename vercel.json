{
    "version": 2,
    "builds": [
        {
            "src": "manhattan/books_backend/books_backend/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
          "src": "build.sh",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "staticfiles"
          }}
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "manhattan/$1"
      },
      {
        "src": "/(.*)",
        "dest": "manhattan/books_backend/books_backend/wsgi.py"
      }
    ]
  }